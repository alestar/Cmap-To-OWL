        <concept-list>
            <concept id="1" label="Photosynthesis">
                <sense domains="[chemistry]"
                    gloss="synthesis of compounds with the aid of radiant energy (especially in plants)"
                    number="09705163" pos="n" words="[photosynthesis]"/>
            </concept>
            <concept id="2" label="Chemical energy">
                <sense domains="[chemistry, physics]"
                    gloss="that part of the energy in a substance that can be released by a chemical reaction"
                    number="07785201" pos="n" words="[chemical_energy]"/>
            </concept>
            <concept id="3" label="Solar energy">
                <sense domains="[physics]"
                    gloss="energy from the sun that is converted into thermal or electrical energy"
                    number="07836720" pos="n" words="[solar_energy, solar_power]"/>
            </concept>
            <concept id="4" label="Chloroplasts"/>
            <concept id="5" label="Green plants"/>
            <concept id="6" label="Chlorophyll">
                <sense domains="[color]"
                    gloss="any of a group of green pigments found in photosynthetic organisms"
                    number="00925109" pos="n" words="[chlorophyll, chlorophyl]"/>
            </concept>
            <concept id="7" label="Different enzyme systems"/>
            <concept id="8" label="Light dependant stage"/>
            <concept id="9" label="Light independant stage"/>
            <concept id="10" label="Light">
                <sense domains="[chemistry, physics]"
                    gloss="(physics, chemistry) not having atomic weight greater than average; &quot;light water is ordinary water&quot;"
                    number="01142009" pos="a" words="[light]"/>
            </concept>
            <concept id="11" label="Water">
                <sense domains="[chemistry, geography]"
                    gloss="a clear colorless odorless tasteless liquid; &quot;he asked for a drink of water&quot;"
                    number="10650211" pos="n" words="[water, H2O]"/>
            </concept>
            <concept id="12" label="Hydrogen">
                <sense domains="[chemistry, geography, physics]"
                    gloss="a nonmetallic univalent element that is normally a colorless and odorless highly flammable diatomic gas; the simplest and lightest and most abundant element in the universe"
                    number="10488691" pos="n" words="[hydrogen, H, atomic_number_1]"/>
            </concept>
            <concept id="13" label="Oxygen">
                <sense domains="[chemistry, geography]"
                    gloss="a nonmetallic bivalent element that is normally a colorless odorless tasteless nonflammable diatomic gas; constitutes 28 percent of the atmosphere by volume; the most abundant element in the earth's crust"
                    number="10495237" pos="n" words="[oxygen, O, atomic_number_8]"/>
            </concept>
            <concept id="14" label="By-product">
                <sense domains="[factotum]"
                    gloss="a secondary and sometimes unexpected consequence"
                    number="07767197" pos="n" words="[by-product]"/>
                <sense domains="[economy, industry]"
                    gloss="made during the manufacture of something else"
                    number="02361643" pos="n" words="[by-product, spin-off]"/>
            </concept>
            <concept id="15" label="Carbon dioxide">
                <sense domains="[chemistry]"
                    gloss="a heavy odorless colorless gas formed during respiration and by the decomposition of organic substances; absorbed from the air by plants in photosynthesis"
                    number="10613550" pos="n" words="[carbon_dioxide, CO, carbonic_acid_gas]"/>
            </concept>
            <concept id="16" label="Carbohydrates"/>
            <concept id="17" label="Darkness">
