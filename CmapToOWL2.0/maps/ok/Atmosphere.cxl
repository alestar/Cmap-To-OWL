    <map root-id="1">
        <concept-list>
            <concept id="1" label="Atmosphere">
                <sense domains="[meteorology]"
                    gloss="the weather or climate at some place; &quot;the atmosphere was thick with fog&quot;"
                    number="10414112" pos="n" words="[atmosphere, atmospheric_state]"/>
            </concept>
            <concept id="2" label="Gases">
                <sense domains="[physics]"
                    gloss="a fluid in the gaseous state having neither independent shape nor volume and being able to expand indefinitely"
                    number="10671811" pos="n" words="[gas]"/>
            </concept>
            <concept id="3" label="Oxygen">
                <sense domains="[chemistry, geography]"
                    gloss="a nonmetallic bivalent element that is normally a colorless odorless tasteless nonflammable diatomic gas; constitutes 28 percent of the atmosphere by volume; the most abundant element in the earth's crust"
                    number="10495237" pos="n" words="[oxygen, O, atomic_number_8]"/>
            </concept>
            <concept id="4" label="Wind">
                <sense domains="[meteorology]"
                    gloss="air moving (sometimes with considerable force) from an area of high pressure to an area of low pressure; &quot;trees bent under the fierce winds&quot;"
                    number="07848889" pos="n" words="[wind]"/>
            </concept>
            <concept id="5" label="Thunderstorms">
                <sense domains="[meteorology]"
                    gloss="a storm resulting from strong rising air currents; heavy rain or hail along with thunder and lightning"
                    number="07843377" pos="n" words="[thunderstorm, electrical_storm, electric_storm]"/>
            </concept>
            <concept id="6" label="Northern lights">
                <sense domains="[meteorology]"
                    gloss="the aurora of the northern hemisphere"
                    number="07777421" pos="n" words="[aurora_borealis, northern_lights]"/>
            </concept>
            <concept id="7" label="Ultraviolet radiation">
                <sense domains="[physics]"
                    gloss="radiation lying in the ultraviolet range; wave lengths shorter than light but longer than X rays"
                    number="07845712" pos="n" words="[ultraviolet, ultraviolet_radiation, ultraviolet_light, ultraviolet_illumination, UV]"/>
            </concept>
            <concept id="8" label="Rainbow">
                <sense domains="[astronomy]"
                    gloss="an arc of colored light in the sky caused by refraction of the sun's rays by rain"
                    number="06747305" pos="n" words="[rainbow]"/>
            </concept>
            <concept id="9" label="Air pressure">
                <sense domains="[physics]"
                    gloss="the pressure exerted by the atmosphere"
                    number="07825969" pos="n" words="[atmospheric_pressure, air_pressure]"/>
            </concept>
            <concept id="10" label="Small space"/>
            <concept id="11" label="Sun shine"/>
            <concept id="12" label="Rain">
                <sense domains="[meteorology]"
                    gloss="water falling in drops from vapor condensed in the atmosphere"
                    number="07830260" pos="n" words="[rain, rainfall]"/>
            </concept>
            <concept id="13" label="Colors">
                <sense domains="[art]"
                    gloss="a flag that shows its nationality"
                    number="02472674" pos="n" words="[colors, colours]"/>
                <sense domains="[art]"
                    gloss="a distinguishing emblem; &quot;his tie proclaimed his school colors&quot;"
                    number="02472788" pos="n" words="[colors, colours]"/>
