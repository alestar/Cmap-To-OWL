            </concept>
            <concept id="30" label="Oil">
                <sense domains="[chemistry]"
                    gloss="a slippery or viscous liquid or liquefiable substance not miscible with water"
                    number="10738422" pos="n" words="[oil]"/>
                <sense domains="[painting]"
                    gloss="paint used by an artist" number="03053864"
                    pos="n" words="[oil, oil_color]"/>
                <sense domains="[factotum]"
                    gloss="cover with oil, as if by rubbing; &quot;oil the wooden surface&quot;"
                    number="00865480" pos="v" words="[oil]"/>
                <sense domains="[transport]"
                    gloss="apply lubricants to' &quot;lubricate my car engine&quot;"
                    number="00335085" pos="v" words="[lubricate, lube, oil]"/>
                <sense domains="[factotum]"
                    gloss="administer a sacred oil or ointment to in a religious ceremony of blessing"
                    number="00058325" pos="v" words="[anoint, oil, anele, ambrocate]"/>
            </concept>
